{
  "mcpServers": {
    "firecrawl": {
      "command": "npx",
      "args": [
        "-y",
        "@mendable/firecrawl-mcp-server"
      ],
      "env": {
        "FIRECRAWL_API_KEY": "fc-c6c470036718491484dfa3b5d8c4d14d"
      }
    }
  },
  "scraping_config": {
    "target_url": "https://68tt.co",
    "crawl_options": {
      "generateImgAltText": true,
      "maxDepth": 2,
      "limit": 10,
      "allowBackwardCrawling": false,
      "allowExternalContentLinks": false,
      "includeRawHtml": true,
      "waitFor": 2000
    },
    "scrape_options": {
      "formats": [
        "markdown",
        "html",
        "rawHtml"
      ],
      "onlyMainContent": false,
      "removeBase64Images": false,
      "includeImages": true,
      "includeLinks": true
    },
    "output": {
      "directory": "./firecrawl_output",
      "formats": {
        "save_html": true,
        "save_markdown": true,
        "save_json": true,
        "save_assets": true,
        "save_screenshots": true
      }
    }
  }
}